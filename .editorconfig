# editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
trim_trailing_whitespace = true
max_line_length = 100

[Makefile]
indent_style = tab
indent_size = 8

[*.rs]
indent_size = 4

[*.s]
indent_style = tab
indent_size = 8

[*.toml]
indent_size = 4
